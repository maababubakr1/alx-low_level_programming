#include "lists.h"

/**
 *size_t - A function that prints all the elements of a listint_t list.
 *@h: linked list to be printed
 *Return: The number of the nodes
 */

size_t print_listint(const listint_t *h)
{
	size_t counter = 0;

	while (h)
	{
		printf("%d\n", h->n);
		counter++;
		h = h->next;
	}
	return (counter);
}
