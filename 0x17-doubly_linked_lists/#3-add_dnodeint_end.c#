#include "lists.h"

/**
 *add_dnodeint_end - function that adds new node at the end of the list
 *@head: pointer to the head of the list
 *@n: data to be stored in the new node
 *Return: the address of the new element, or NULL if it failed
 */

dlistint_t *add_dnodeint_end(dlistint_t **head, const int n)
{
	dlistint_t *new, *end;

	new = malloc(sizeof(dlistint_t));
	if (new == NULL)
		return (NULL);

	new->n = n;
	new->next = NULL;
	if (*head == NULL)
	{
		new->prev = NULL;
		*head = new;
		return (new);
	}

	NULL)
		end = end->next;
	end->next = new;
	new->prev = end;

	return (new);
}
