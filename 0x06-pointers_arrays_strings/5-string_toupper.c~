#include "main.h"

/**
 *string_toupper - A function that changes all lowercase letters to uppercase.
 *@s: A string to modify.
 *Return: the changed string
 */

char *string_toupper(char *)
{
	int i;
	char *s;

	for (i = 0; s[i] != '\0'; i++)
		if (s[i] >= 'a' && s[i] <= 'z')
			s[i] = s[i] - 32;
	return (s);
}
